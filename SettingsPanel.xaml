<UserControl x:Class="PasswordManager.SettingsPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PasswordManager"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBox x:Name="1PasswordEmail" Text="1Password Email"></TextBox>
        <TextBox x:Name="1PasswordDomain" Text="1Password Domain"></TextBox>
        <TextBox x:Name="1PasswordSecretKey" Text="1Password Secret Key"></TextBox>
        <TextBox x:Name="1PasswordPassword" Text="1Password Password"></TextBox>
        <TextBox x:Name="1Password2FA" Text="1Password 2FA Code"></TextBox>
        <Textbox x:Name="1PasswordVault" Text="1Password Vault"></Textbox>
    </Grid>
</UserControl>
